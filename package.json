{
  "name": "my-project",
  "version": "0.0.1",
  "scripts": {
    "bemo": "npm run bemo:sprites && npm run bemo:webfonts",
    "bemo:sprites": "bemo-sprites -s source/images/sprites -i source/images -S source/stylesheets/variables/_sprites.scss",
    "bemo:webfonts": "bemo-webfonts -s source/fonts/svg -S source/stylesheets/variables/_icon-glyphs.scss -f source/fonts",
    "bemo:scaffold": "bemo-scaffold -s source/stylesheets"
  },
  "devDependencies": {
    "bemo-scaffold": "*",
    "bemo-sprites": "*",
    "bemo-webfonts": "*"
  }
}
